{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 11
	},
	"rules": {
		"no-mixed-spaces-and-tabs": "off",
		"no-unused-vars": "warn",
		"no-extra-semi": "warn"
	}
}
